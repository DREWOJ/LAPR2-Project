@startuml US04_DM
hide methods
left to right direction
skinparam classAttributeIconSize 0

class Receptionist {
  - name
  - phoneNumber
  - email
  - password
  - address
  - citizenCardNumber
  - roleId
}

class Arrival {
  - date
}

class Appointment {}

class SNSUser {
  - citizenCard
  - snsNumber
  - name
  - birthDay
  - gender
  - phoneNumber
  - email
  - address
}

class Company {
  - designation
}

Arrival "1" -- "0..1" Appointment : related to >
Company "1" -- "*" Receptionist : knows >
Company "1" -- "*" SNSUser : knows >
Receptionist "1" -- "*" Arrival : registers >
SNSUser "1" -- "*" Arrival : associated to >

@enduml

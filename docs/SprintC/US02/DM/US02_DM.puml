@startuml  US02_DM
hide methods
skinparam classAttributeIconSize 0


class Appointment {}

class VaccineType {}

class SNSUser {
  - citizenCard
  - snsNumber
  - name
  - birthDay
  - gender
  - phoneNumber
  - email
  - address
}

class VaccinationCenter{
  -name
  -address
  -emailAddress
  -phoneNumber
  -faxNumber
  -websiteAddress
  -openingHours  
  -closingHours
  -slotDuration
  -maximumVaccinesPerSlot
  -coordinator
}

class Company{
  -designation
}

class Receptionist {}

class Employee {}

Company "1" -- "*" Appointment : knows >
Appointment "1"  -- "1" VaccineType : has >
Appointment "*" -- "1" VaccinationCenter : has >
Company "1" -- "*" VaccinationCenter : knows >
Appointment -- Receptionist : associated to >
Receptionist "1" -- "1" SNSUser : schedules for >
Company "1" -- "*" Receptionist : knows >
Appointment "1" -- "1" SNSUser : has >
Employee <|-- Receptionist
Company -- Employee : knows >


@enduml
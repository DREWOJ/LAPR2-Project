@startuml  US02_DM
hide methods
skinparam classAttributeIconSize 0


class VaccinationSchedule {}

class VaccineType {}

class SNSUser {
  - citizenCard
  - snsNumber
  - name
  - birthDay
  - gender
  - phoneNumber
  - email
  - address
}

class VaccinationCenter{
  -name
  -address
  -emailAddress
  -phoneNumber
  -faxNumber
  -websiteAddress
  -openingHours  
  -closingHours
  -slotDuration
  -maximumVaccinesPerSlot
  -coordinator
}

class Company{
  -designation
}

class Receptionist {}

class Employee {}

Company "1" -- "*" VaccinationSchedule : knows >
VaccinationSchedule "1"  -- "1" VaccineType : has >
VaccinationSchedule "*" -- "1" VaccinationCenter : related to <
Company "1" -- "*" VaccinationCenter : knows >
VaccinationSchedule -- Receptionist : associated to >
Receptionist "1" -- "1" SNSUser : schedules for >
Company "1" -- "*" Receptionist : knows >
VaccinationSchedule "1" -- "1" SNSUser : has >
Employee <|-- Receptionist
Company -- Employee : has >


@enduml
@startuml US01_DM
hide methods
skinparam classAttributeIconSize 0

class VaccinationSchedule {}

class VaccineType {}

class SNSUser {
  -citizenCard
  -snsNumber
  -name
  -birthDay
  -gender
  -phoneNumber
  -email
  -address
}

class VaccinationCenter{
  -name
  -address
  -emailAddress
  -phoneNumber
  -faxNumber
  -websiteAddress
  -openingHours  
  -closingHours
  -slotDuration
  -maximumVaccinesPerSlot
  -coordinator
}

class Company{
  -designation
}

Company "1" -- "*" VaccinationSchedule : knows >
VaccinationSchedule "1"  -- "1" VaccineType : has >
VaccinationSchedule "*" -- "1" VaccinationCenter : has >
Company "1" -- "*" VaccinationCenter : knows >
VaccinationSchedule "1" -- "1" SNSUser : schedules <
Company "1" -- "*" SNSUser : knows >

@enduml